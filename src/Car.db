BEGIN TRANSACTION;
CREATE TABLE booking(
ID VARCHAR(4) NOT NULL PRIMARY KEY,
User_name VARCHAR(40) REFERENCES user(name) NOT NULL,
Plate_Number VARCHAR(8) REFERENCES cars(Plate_Number) NOT NULL,
Stock_Name VARCHAR(20) REFERENCES stocks(Name) NOT NULL,
Booking_Date DATE NOT NULL,
others VARCHAR(50) NOT NULL
);
CREATE TABLE stocks(
Name VARCHAR(20) NOT NULL PRIMARY KEY,
Quantity VARCHAR(2) NOT NULL,
Price DOUBLE(4,2) NOT NULL
);
INSERT INTO `stocks` VALUES('Shell Helix Ultra',5,'225.0');
CREATE TABLE PAYMENT(
ID VARCHAR(4) NOT NULL PRIMARY KEY,
Name VARCHAR(40) NOT NULL REFERENCES user(name),
Plate_Number VARCHAR(8) NOT NULL REFERENCES cars(Plate_Number),
Payment_Date DATE NOT NULL,
Total DECIMAL(8,2) NOT NULL
);
CREATE TABLE user(
username VARCHAR(10) NOT NULL PRIMARY KEY,
Plate_Number VARCHAR(8) REFERENCES CARS(Plate_Number),
password VARCHAR(6) NOT NULL,
name VARCHAR(40) NOT NULL,
IC VARCHAR(12) NOT NULL,
address VARCHAR(70) NOT NULL,
phone VARCHAR(10) NOT NULL,
privilage  VARCHAR(1) NOT NULL
);
